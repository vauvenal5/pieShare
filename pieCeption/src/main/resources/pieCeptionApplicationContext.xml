<?xml version="1.0" encoding="windows-1252"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
">
    <import resource="classpath*:piePlateApplicationContext.xml" />
    
    <bean id="clusterConnectorService" class="org.pieShare.pieTools.pieCeption.service.core.ClusterConnectorService">
        <property name="clusterService" ref="clusterService"/>
    </bean>  
    
    <bean id="pieCeptionService" class="org.pieShare.pieTools.pieCeption.service.core.PieCeptionService">
        <property name="connectorService" ref="clusterConnectorService"/>
        <property name="executorService" ref="pieExecutorService"/>
    </bean>
</beans>
